{
    "version": 2,
    "projects": {
        "authorization": {
            "root": "apps/authorization",
            "sourceRoot": "apps/authorization",
            "projectType": "application",
            "targets": {
                "build": {
                    "executor": "@nrwl/next:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "root": "apps/authorization",
                        "outputPath": "dist/apps/authorization"
                    },
                    "configurations": {
                        "production": {}
                    }
                },
                "serve": {
                    "executor": "@nrwl/next:server",
                    "options": {
                        "buildTarget": "authorization:build",
                        "dev": true
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "authorization:build:production",
                            "dev": false
                        }
                    }
                },
                "export": {
                    "executor": "@nrwl/next:export",
                    "options": {
                        "buildTarget": "authorization:build:production"
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/authorization"],
                    "options": {
                        "jestConfig": "apps/authorization/jest.config.js",
                        "passWithNoTests": true
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": [
                            "apps/authorization/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                }
            }
        },
        "authorization-e2e": {
            "root": "apps/authorization-e2e",
            "sourceRoot": "apps/authorization-e2e/src",
            "projectType": "application",
            "targets": {
                "e2e": {
                    "executor": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/authorization-e2e/cypress.json",
                        "devServerTarget": "authorization:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "authorization:serve:production"
                        }
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": [
                            "apps/authorization-e2e/**/*.{js,ts}"
                        ]
                    }
                }
            }
        },
        "client": {
            "root": "apps/client",
            "sourceRoot": "apps/client",
            "projectType": "application",
            "targets": {
                "build": {
                    "executor": "@nrwl/next:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "root": "apps/client",
                        "outputPath": "dist/apps/client"
                    },
                    "configurations": {
                        "production": {}
                    }
                },
                "serve": {
                    "executor": "@nrwl/next:server",
                    "options": {
                        "buildTarget": "client:build",
                        "dev": true
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "client:build:production",
                            "dev": false
                        }
                    }
                },
                "export": {
                    "executor": "@nrwl/next:export",
                    "options": {
                        "buildTarget": "client:build:production"
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/client"],
                    "options": {
                        "jestConfig": "apps/client/jest.config.js",
                        "passWithNoTests": true
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": ["apps/client/**/*.{ts,tsx,js,jsx}"]
                    }
                }
            }
        },
        "client-e2e": {
            "root": "apps/client-e2e",
            "sourceRoot": "apps/client-e2e/src",
            "projectType": "application",
            "targets": {
                "e2e": {
                    "executor": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/client-e2e/cypress.json",
                        "devServerTarget": "client:serve",
                        "tsConfig": "apps/client-e2e/tsconfig.json"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "client:serve:production"
                        }
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": ["apps/client-e2e/**/*.{js,ts}"]
                    }
                }
            }
        },
        "shared-auth": {
            "root": "libs/shared/auth",
            "sourceRoot": "libs/shared/auth/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": [
                            "libs/shared/auth/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/shared/auth"],
                    "options": {
                        "jestConfig": "libs/shared/auth/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "shared-ui-components": {
            "root": "libs/shared/ui-components",
            "sourceRoot": "libs/shared/ui-components/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": [
                            "libs/shared/ui-components/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/shared/ui-components"],
                    "options": {
                        "jestConfig": "libs/shared/ui-components/jest.config.js",
                        "passWithNoTests": true
                    }
                },
                "storybook": {
                    "executor": "@nrwl/storybook:storybook",
                    "options": {
                        "uiFramework": "@storybook/react",
                        "port": 4400,
                        "config": {
                            "configFolder": "libs/shared/ui-components/.storybook"
                        }
                    },
                    "configurations": {
                        "ci": {
                            "quiet": true
                        }
                    }
                },
                "build-storybook": {
                    "executor": "@nrwl/storybook:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "uiFramework": "@storybook/react",
                        "outputPath": "dist/storybook/shared-ui-components",
                        "config": {
                            "configFolder": "libs/shared/ui-components/.storybook"
                        }
                    },
                    "configurations": {
                        "ci": {
                            "quiet": true
                        }
                    }
                }
            }
        },
        "shared-ui-components-e2e": {
            "root": "apps/shared-ui-components-e2e",
            "sourceRoot": "apps/shared-ui-components-e2e/src",
            "projectType": "application",
            "targets": {
                "e2e": {
                    "executor": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/shared-ui-components-e2e/cypress.json",
                        "devServerTarget": "shared-ui-components:storybook",
                        "tsConfig": "apps/shared-ui-components-e2e/tsconfig.json"
                    },
                    "configurations": {
                        "ci": {
                            "devServerTarget": "shared-ui-components:storybook:ci"
                        }
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": [
                            "apps/shared-ui-components-e2e/**/*.{js,ts}"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/next"
    },
    "generators": {
        "@nrwl/react": {
            "application": {
                "style": "@emotion/styled",
                "linter": "eslint",
                "babel": true
            },
            "component": {
                "style": "@emotion/styled"
            },
            "library": {
                "style": "@emotion/styled",
                "linter": "eslint"
            }
        },
        "@nrwl/next": {
            "application": {
                "style": "@emotion/styled",
                "linter": "eslint"
            }
        }
    },
    "defaultProject": "client"
}
